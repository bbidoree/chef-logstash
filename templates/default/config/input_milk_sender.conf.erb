
input {
	file {
		type => "serverLog"
		path => "/opt/wildfly/standalon/log/server*"
		start_position => "beginning"
		codec => multiline {
			pattern => "^%{TIMESTAMP_ISO8601} "
			negate => true
			what => previous
			}
	}

	file {
		type => "sgmpLog"
		path => "/opt/wildfly/standalon/log/sgmp/sgmp*"
		start_position => "beginning"
		codec => multiline {
			pattern => "^\["
			negate => true
			what => previous
			}
	}

